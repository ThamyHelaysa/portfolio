---
layout: base.webc
---

<article class="post">
  <header>
    <h1 class="post-title" @text="this.title"></h1>

    <p class="post-meta" webc:if="this.date">
      <time @text="this.date"></time>
    </p>
  </header>

  <section class="post-content">
    <template @html="this.content" webc:nokeep></template>
  </section>
</article>
